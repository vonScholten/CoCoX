<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ConstraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Background_lightgrey"
    tools:context="gr04.cocox.MoodActivity">


    <ImageButton
        android:id="@+id/imageButton20"
        android:layout_width="0dp"
        android:layout_height="98dp"
        android:layout_marginBottom="41dp"
        android:layout_marginEnd="72dp"
        android:layout_marginStart="42dp"
        android:layout_marginTop="43dp"
        android:background="@color/Background_lightgrey"
        app:layout_constraintBottom_toBottomOf="@+id/bored"
        app:layout_constraintEnd_toStartOf="@+id/bored"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_return1" />


    <ImageButton
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="24dp"
        android:background="@color/Background_lightgrey"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/happy"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/keyboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="21dp"
        android:background="@color/Background_lightgrey"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/worried"
        app:srcCompat="@drawable/keyboard" />

    <ImageButton
        android:id="@+id/call"
        android:layout_width="0dp"
        android:layout_height="84dp"
        android:layout_marginBottom="35dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="34dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="1.90"
        android:scaleY="1.60"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/breath"
        app:srcCompat="@drawable/call" />

    <ImageButton
        android:id="@+id/bored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:layout_marginTop="6dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintEnd_toStartOf="@+id/cold"
        app:layout_constraintStart_toEndOf="@+id/imageButton20"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bored" />

    <ImageButton
        android:id="@+id/cold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="6dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintEnd_toStartOf="@+id/content"
        app:layout_constraintStart_toEndOf="@+id/bored"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cold" />

    <ImageButton
        android:id="@+id/content"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="42dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintBottom_toBottomOf="@+id/frustrated"
        app:layout_constraintEnd_toStartOf="@+id/frustrated"
        app:layout_constraintStart_toEndOf="@+id/cold"
        app:layout_constraintTop_toTopOf="@+id/frustrated"
        app:srcCompat="@drawable/content" />

    <ImageButton
        android:id="@+id/frustrated"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="215dp"
        android:layout_marginTop="6dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/content"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/frustrated" />

    <ImageButton
        android:id="@+id/happy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/bored"
        app:layout_constraintTop_toBottomOf="@+id/bored"
        app:srcCompat="@drawable/happy" />

    <ImageButton
        android:id="@+id/scared"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/cold"
        app:layout_constraintTop_toBottomOf="@+id/cold"
        app:srcCompat="@drawable/scared" />

    <ImageButton
        android:id="@+id/sore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/frustrated"
        app:layout_constraintTop_toBottomOf="@+id/frustrated"
        app:srcCompat="@drawable/sore" />

    <ImageButton
        android:id="@+id/tired"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/content"
        app:layout_constraintTop_toBottomOf="@+id/content"
        app:srcCompat="@drawable/tired" />

    <ImageButton
        android:id="@+id/worried"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/happy"
        app:layout_constraintTop_toBottomOf="@+id/happy"
        app:srcCompat="@drawable/worried" />

    <ImageButton
        android:id="@+id/varm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/scared"
        app:layout_constraintTop_toBottomOf="@+id/scared"
        app:srcCompat="@drawable/warm" />

    <ImageButton
        android:id="@+id/thirsty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/tired"
        app:layout_constraintTop_toBottomOf="@+id/tired"
        app:srcCompat="@drawable/thirsty" />

    <ImageButton
        android:id="@+id/breath"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:background="@color/Background_lightgrey"
        android:scaleType="fitCenter"
        android:scaleX="0.90"
        android:scaleY="0.90"
        app:layout_constraintStart_toStartOf="@+id/sore"
        app:layout_constraintTop_toBottomOf="@+id/sore"
        app:srcCompat="@drawable/breath" />

</android.support.constraint.ConstraintLayout>

